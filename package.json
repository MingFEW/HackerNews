{
  "name": "reactjs-starter-with-vite",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "clean": "rimraf yarn.lock package-lock.json node_modules build",
    "check-types": "tsc --noEmit --pretty",
    "lint": "eslint . \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint . \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "prettier:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "prepare": "husky install"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "i18next": "^22.0.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^12.0.0",
    "react-icons": "^4.7.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-loading-skeleton": "^3.1.0",
    "react-modal": "^3.16.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "redux-injectors": "^2.1.0",
    "redux-saga": "^1.2.1",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@pancakeswap/eslint-config-pancake": "1.2.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-modal": "^3.13.1",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^7.2.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "twin.macro": "^3.0.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
